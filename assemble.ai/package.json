{
  "name": "temp-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n next,doc-worker,draw-worker -c blue,magenta,cyan \"next dev\" \"tsx watch workers/document-processor/index.ts\" \"tsx watch workers/drawing-extractor/index.ts\"",
    "dev:next": "next dev",
    "dev:local": "dotenv -e .env.development -- next dev",
    "build": "next build && npm run worker:build",
    "start": "concurrently -n next,worker \"node server.js\" \"node dist/workers/index.js\"",
    "start:next": "next start",
    "lint": "eslint",
    "test": "jest",
    "worker:dev": "tsx workers/document-processor/index.ts",
    "worker:drawing": "tsx workers/drawing-extractor/index.ts",
    "worker:build": "tsc workers/document-processor/index.ts --outDir dist/workers --module commonjs --esModuleInterop --skipLibCheck",
    "worker:start": "node dist/workers/document-processor/index.js",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:push": "drizzle-kit push --config=drizzle.pg.config.ts --force",
    "db:generate": "drizzle-kit generate --config=drizzle.pg.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.pg.config.ts",
    "db:rag:generate": "drizzle-kit generate --config=drizzle.rag.config.ts",
    "db:rag:push": "drizzle-kit push --config=drizzle.rag.config.ts",
    "db:rag:studio": "drizzle-kit studio --config=drizzle.rag.config.ts",
    "db:auth:generate": "drizzle-kit generate --config=drizzle.auth.config.ts",
    "db:auth:push": "drizzle-kit push --config=drizzle.auth.config.ts --force",
    "db:auth:studio": "drizzle-kit studio --config=drizzle.auth.config.ts",
    "db:auth:migrate-users": "tsx scripts/migrate-users-to-better-auth.ts",
    "db:auth:test": "tsx scripts/test-better-auth.ts"
  },
