  "objectives_functional_quality": [
    {
      "id": "ofq-001",
      "description": "Bedroom count from scale",
      "condition": { "profiler": { "building_class": "residential", "scale": { "bedrooms": { "min": 1 } } } },
      "infer": [{ "text": "{{scale.bedrooms}} bedroom accommodation", "confidence": "high" }],
      "priority": 100,
      "source": "explicit"
    },
    {
      "id": "ofq-002",
      "description": "Multi-storey construction",
      "condition": { "profiler": { "scale": { "storeys": { "min": 2 } } } },
      "infer": [{ "text": "{{scale.storeys}}-storey construction", "confidence": "high" }],
      "priority": 90,
      "source": "explicit"
    },
    {
      "id": "ofq-003",
      "description": "Quality tier finishes",
      "condition": { "profiler": { "complexity": { "quality_tier": ["medium", "high", "premium"] } } },
      "infer": [{ "text": "{{complexity.quality_tier}} quality finishes", "confidence": "high" }],
      "priority": 80,
      "source": "explicit"
    },
    {
      "id": "ofq-010",
      "description": "Infill site considerations",
      "condition": { "profiler": { "complexity": { "site_conditions": "infill" } } },
      "infer": [
        { "text": "Boundary setback compliance", "category": "Site", "confidence": "high" },
        { "text": "Party wall considerations", "category": "Site", "confidence": "medium" },
        { "text": "Overshadowing assessment", "category": "Site", "confidence": "medium" }
      ],
      "priority": 70,
      "source": "inferred"
    },
    {
      "id": "ofq-011",
      "description": "Heritage facade considerations",
      "condition": { "profiler": { "complexity": { "heritage": "heritage_overlay" } } },
      "infer": [
        { "text": "Heritage facade retention", "category": "Heritage", "confidence": "high" },
        { "text": "Sympathetic material selection", "category": "Heritage", "confidence": "medium" }
      ],
      "priority": 75,
      "source": "inferred"
    },
    {
      "id": "ofq-020",
      "description": "Premium residential amenities",
      "condition": {
        "and": [
          { "profiler": { "building_class": "residential" } },
          { "profiler": { "complexity": { "quality_tier": "premium" } } }
        ]
      },
      "infer": [
        { "text": "High-end fixtures and fittings", "category": "Finishes", "confidence": "high" },
        { "text": "Engineered stone benchtops", "category": "Finishes", "confidence": "medium" },
        { "text": "Custom joinery throughout", "category": "Finishes", "confidence": "medium" }
      ],
      "priority": 60,
      "source": "inferred"
    }
  ],

  "objectives_planning_compliance": [
    {
      "id": "opc-001",
      "description": "Building code classification (AU)",
