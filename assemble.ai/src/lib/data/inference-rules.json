      "priority": 100,
      "source": "explicit"
    },
    {
      "id": "opc-002",
      "description": "Approval pathway",
      "condition": { "profiler": { "complexity": { "approval_pathway": ["standard_da", "regional_da", "state_significant", "complying_dev"] } } },
      "infer": [{ "text": "{{complexity.approval_pathway}} approval", "confidence": "high" }],
      "priority": 95,
      "source": "explicit"
    },
    {
      "id": "opc-010",
      "description": "Heritage overlay requirements",
      "condition": { "profiler": { "complexity": { "heritage": "heritage_overlay" } } },
      "infer": [
        { "text": "Heritage consultant certification", "category": "Heritage", "confidence": "high" },
        { "text": "Conservation management plan", "category": "Heritage", "confidence": "high" },
        { "text": "Heritage impact statement", "category": "Heritage", "confidence": "medium" }
      ],
      "priority": 85,
      "source": "inferred"
    },
    {
      "id": "opc-011",
      "description": "Bushfire overlay requirements (AU)",
      "condition": {
        "and": [
          { "profiler": { "region": "AU" } },
          { "profiler": { "complexity": { "bushfire": ["bap", "fz"] } } }
        ]
      },
      "infer": [
        { "text": "AS 3959 bushfire compliance", "category": "Bushfire", "confidence": "high" },
        { "text": "Bushfire attack level assessment", "category": "Bushfire", "confidence": "high" },
        { "text": "Bushfire management plan", "category": "Bushfire", "confidence": "medium" }
      ],
      "priority": 80,
      "source": "inferred"
    },
    {
      "id": "opc-012",
      "description": "Contamination requirements",
      "condition": { "profiler": { "complexity": { "contamination_level": ["minor", "significant", "heavily_contaminated"] } } },
      "infer": [
        { "text": "Detailed site investigation", "category": "Environmental", "confidence": "high" },
        { "text": "Remediation action plan", "category": "Environmental", "confidence": "high" },
        { "text": "Site auditor sign-off", "category": "Environmental", "confidence": "medium" }
      ],
      "priority": 75,
      "source": "inferred"
    },
    {
      "id": "opc-020",
      "description": "NSW BASIX requirement",
      "condition": {
        "and": [
          { "profiler": { "region": "AU" } },
          { "profiler": { "building_class": ["residential", "mixed"] } }
        ]
      },
      "infer": [{ "text": "BASIX certification", "category": "Sustainability", "confidence": "high" }],
      "priority": 70,
      "source": "inferred"
    },
    {
      "id": "opc-021",
      "description": "State significant development",
      "condition": { "profiler": { "complexity": { "approval_pathway": "state_significant" } } },
      "infer": [
        { "text": "Environmental Impact Statement", "category": "Planning", "confidence": "high" },
        { "text": "Community consultation", "category": "Planning", "confidence": "high" },
        { "text": "Response to submissions", "category": "Planning", "confidence": "high" }
      ],
      "priority": 90,
      "source": "inferred"
    }
